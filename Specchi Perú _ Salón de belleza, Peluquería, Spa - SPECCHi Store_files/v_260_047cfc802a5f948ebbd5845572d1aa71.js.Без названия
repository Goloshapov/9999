function htmlify(text){if(!text){return"";}
return $('<span />').text(text).html();}
function isMsie(){return navigator.cpuClass&&!navigator.product;}
function isMsieSevenOrNewer(){if(wind.XMLHttpRequest!==undefined){return true;}
